{
  "schema_version": 1,
  "quilt_loader": {
    "group": "com.example",
    "id": "${{ mod_id }}",
    "version": "${version}",
    "metadata": {
      "name": "${{ mod_name }}",
      "description": "${{ mod_description }}",
      "contributors": {
        "${{ mod_owner }}": "Owner"
      },
      "contact": {
        "sources": "${{ mod_sources }}",
        "issues": "${{ mod_sources }}/issues"
      },
      "license": "${{ mod_license }}",
      "icon": "assets/${{ mod_id }}/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": "com.example.${{ mod_id }}.${{ main_class }}"
    },
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=0.17.0-"
      },
      {
        "id": "quilted_fabric_api",
        "versions": ">=4.0.0-"
      },
      {
        "id": "minecraft",
        "versions": ">=1.19"
      }
    ]
  },
  "mixin": "${{ mod_id }}.mixins.json"
}
